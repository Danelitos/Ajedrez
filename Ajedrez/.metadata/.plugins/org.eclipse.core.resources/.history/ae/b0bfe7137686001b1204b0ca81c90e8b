package org.pmoo.packlaboratorio6;

public class ComplementoCargo extends Complementos {
	//Atributos
	private String descripcion;
	
	//Constructora
	public ComplementoCargo(String pDescripcion, double pImporteBruto) {
		super(20, pImporteBruto);
		this.descripcion=pDescripcion;
	}
	
	public double calcularCantidad() {
		return super.calcularCantidad();
	}
}
